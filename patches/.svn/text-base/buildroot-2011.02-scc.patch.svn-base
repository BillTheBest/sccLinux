++/**
++ * \brief Read and display ethernet statistics
++ */
++void show_statistic(void) {
++	int i;
++
++	printk("Ethernet statistic: \n");
++	printk("----------------------------\n");
++	for (i = 0; i < 46; i++) {
++		printk("0x%4x\t-\t%4d\n", STAT0_TRBYTE + i * 8,
++               readl(RA(STAT0_TRBYTE + i * 8, 0)));
++	}
++	printk("----------------------------\n");
