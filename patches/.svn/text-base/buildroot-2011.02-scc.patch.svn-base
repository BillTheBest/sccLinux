++		host->flags |= ATA_HOST_PARALLEL_SCAN;
++	else
++		printk(KERN_INFO "ahci: SSS flag set, parallel bus scan disabled\n");
++
++	if (pi.flags & ATA_FLAG_EM)
++		ahci_reset_em(host);
++
++	for (i = 0; i < host->n_ports; i++) {
++		struct ata_port *ap = host->ports[i];
++
++		/* set enclosure management message type */
++		if (ap->flags & ATA_FLAG_EM)
++			ap->em_message_type = hpriv->em_msg_type;
++
++		/* disabled/not-implemented port */
++		if (!(hpriv->port_map & (1 << i)))
++			ap->ops = &ata_dummy_port_ops;
++	}
++	pdev->dev.coherent_dma_mask = DMA_BIT_MASK(32);
++	pdev->dev.dma_mask = &pdev->dev.coherent_dma_mask;
++
++	rc = ahci_reset_controller(host);
++	if (rc)
++		return rc;
++
++	ahci_init_controller(host);
++
++	ahci_print_info(host, "SATA");
++
++	rc = ata_host_start(host);
++	if (rc) {
++		return rc;
++	}
++
++	rc = devm_request_irq(host->dev, irq, rckahci_interrupt, IRQF_SHARED, dev_driver_string(host->dev), host);
++	if (rc) {
++		return rc;
++	}
++
++	sata_clear_interrupt();
++	tmp = readl(RA(IRQ_MASK, core * 2));
++	writel(tmp & ~(SATA_ID), RA(IRQ_MASK, core *2));
++
++
++	for (i = 0; i < host->n_ports; i++) {
++		ata_port_desc(host->ports[i], "irq %d", irq);
++	}
++
++	rc = ata_host_register(host, &ahci_sht);
++	if (rc) {
++		devm_free_irq(host->dev, irq, host);
++	}
++	return rc;
++}
