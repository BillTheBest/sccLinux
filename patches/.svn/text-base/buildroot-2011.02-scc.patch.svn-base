++	if (res_x != 0 && res_y != 0 && res_bpp != 0) {
++		printk(KERN_INFO "rckgfxfb: Valid information from cmdline\n");
++		x = res_x;
++		y = res_y;
++		bpp = res_bpp;
++	} else {
++		printk(KERN_INFO "rckgfxfb: Trying FPGA values\n");
++		/* Read requested display values */
++		shm = ioremap_nocache(grb_offset, 0x10000);
++		if (shm != NULL) {
++			res_x = readl(shm + 0x8238) & 0xFFFFFFFF;
++			res_y = readl(shm + 0x823C) & 0xFFFFFFFF;
++			res_bpp = readl(shm + 0x8240) & 0xFFFFFFFF;
++			if (res_x != 0 && res_y != 0 && res_bpp != 0) {
++				printk(KERN_INFO "rckgfxfb: Valid information from FPGA\n");
++				x = res_x;
++				y = res_y;
++				bpp = res_bpp;
++			}
++			iounmap(shm);
++		} else {
++			printk(KERN_DEBUG "rckgfxfb: could not map grb and therefore not read display "
++                   "values, setting default values (%dx%dx%d)!\n", x, y, bpp);
++		}
++	}
++
