++static const struct net_device_ops rckpc_netdev_ops = {
++	.ndo_open = rckpc_open,
++	.ndo_stop = rckpc_close,
++	.ndo_start_xmit = rckpc_tx,
++	.ndo_tx_timeout = rckpc_tx_timeout,
++	.ndo_change_mtu = rckpc_change_mtu,
++	.ndo_do_ioctl = rckpc_ioctl,
++
++	//.ndo_get_stats = rckpc_config,
++};
