++/*
++ * Rock Creek specific helper functions
++ */
++
++
++/* ip2phys calculates the physical address of the given type for the
++ * core ID
++ */
++const static int _CRB = 0;
++const static int _MPB = 1;
++int rckmb_id2phys(u8 id, int is_mpb)
++{
++  int tile_number = (id/2);
++  int core_number = (id%2);
++  int result;
++  
++  /* Only accept IDs for the available cores */
++  if (id >= RCK_CORECOUNT) return 0;
++  
++  if (is_mpb) {
++    result  = mpb_offset;
++    result += mpb_stride*tile_number;
++    result += RCK_MPB_SIZE*core_number;
++  }
++  else {
++    result  = crb_offset;
++    result += RCK_TILE_SIZE*tile_number;
++  }
++  
++  return PAGE_ALIGN(result);
++}
++
++
++/* Calculate the descriptor address for a packet transfer from the given
++ * sender IP address. Note that the corresponding descriptor data is stored
++ * in the receiver's MPB.
++ */
++int rckmb_get_desc_address(struct net_device* dev, u8 sender_ip, u8 receiver_ip)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         address;
++  
++  /* The packet descriptors are stored at the receiver side i.e. start with
++   * the message passing buffer of the destination
++   */
++  address  = (int)(priv->mpb[receiver_ip-1]);
++  /* The descriptor area is located at the top i.e. skip the data range */
++  address += (RCK_CLINE_SIZE * priv->mpb_buffersize);
++  /* Evaluate the offset corresponding to the sender IP */
++  address += sender_ip-1;
++
++  return address;
++}
++
++
++/* lock/unlock access the LOCK bit in the core register bank of the
++ * given IP address
++ */
++void rckmb_lock(struct net_device* dev, u8 ip_address)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         core_number = ((ip_address-1)%2);
++  int                         lock_address = (int)(priv->crb[ip_address-1]);
++
++  /* Simply ignore the test&set register if locking is disabled */
++  if (disable_locking) return;
++  
++  
++  if (core_number == 0) lock_address += RCK_LOCK0;
++  else                  lock_address += RCK_LOCK1;
++  
++  /* The LOCK bit is clear-on-read i.e. we have exclusive access when
++   * reading a '1'.
++   */
++  while (!(readb((void*)lock_address) & 0x1)) cpu_relax();
++}
++
++void rckmb_unlock(struct net_device* dev, u8 ip_address)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         core_number = ((ip_address-1)%2);
++  int                         lock_address = (int)(priv->crb[ip_address-1]);
++
++  /* Simply ignore the test&set register if locking is disabled */
++  if (disable_locking) return;
++
++  
++  if (core_number == 0) lock_address += RCK_LOCK0;
++  else                  lock_address += RCK_LOCK1;
++  
++  /* The LOCK bit can be set by writing to the register.
++   */
++  writeb(0, (void*)lock_address);
++}
++
++
++/* trigger_irq/clear_irq access the interrupt bit in the core register bank 
++ * of the given IP address which is connected to the processor's INTR pin
++ */
++void rckmb_trigger_irq(struct net_device* dev, u8 ip_address)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         core_number = ((ip_address-1)%2);
++  int                         irq_address = (int)(priv->crb[ip_address-1]);
++  unsigned int                value;
++  
++  if (core_number == 0) irq_address += RCK_GLCFG0;
++  else                  irq_address += RCK_GLCFG1;
++  
++  /* Set the INTR bit */
++  value  = readl((void*)irq_address);
++
++  if ((!edgeIrq) && (value & RCK_INTR_MASK)) {
++    value &= (~(RCK_INTR_MASK|RCK_NMI_MASK));
++    writel(value, (void*)irq_address);
++  }
++
++  value |= RCK_INTR_MASK;
++  writel(value, (void*)irq_address);
++
++  /* In case edge interrupts are configured immediately clear the bit again.
++   * Note that we have to clear all interrupt bits as otherwise we might
++   * accidentally re-set a bit that was already cleared because of race
++   * conditions which would then lead to lost interrupts!
++   */
++  if (edgeIrq) {
++    value &= (~(RCK_INTR_MASK|RCK_NMI_MASK));
++    writel(value, (void*)irq_address);
++  }
++#ifdef DBG_INTERRUPT
++  priv->txIntCount[ip_address - 1]++;
++#endif
++}
++
++
++/* reset interrupt bit for core 
++ */
++void rckmb_clear_irq(struct net_device* dev, u8 ip_address)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         core_number = ((ip_address-1)%2);
++  int                         irq_address = (int)(priv->crb[ip_address-1]);
++  unsigned int                value;
++  
++  if (core_number == 0) irq_address += RCK_GLCFG0;
++  else                  irq_address += RCK_GLCFG1;
++
++  /* Clear the INTR bit */
++  value  = readl((void*)irq_address);
++  value &= (~RCK_INTR_MASK);
++  writel(value, (void*)irq_address);
++}
