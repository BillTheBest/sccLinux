++/* module initialization - called at module load time */
++static int __init rckmem_init(void) {
++  int ret = 0;
++  void* mappedAddr;
++  int pid, x=0, y=0, z=0;
++  int loopMPB;
++
++  PRINTD(KERN_INFO "Starting up Rock Creek memory driver...\n");
++
++  /* Register the requested major number of the character device */
++  if ((ret = register_chrdev_region(MKDEV(MAJOR_ADDR,0), 3, "rckmem")) < 0) {
++    printk(KERN_ERR "rckmem_init(): Could not allocate major number for rckmem!\n");
++    goto out;
++  }
++
++  /* Initialize the device structure and register the "rckncm" device (Minor 0) with the kernel */
++  cdev_init(&rckncm_cdev, &rckncm_fops);
++  if ((ret = cdev_add(&rckncm_cdev, MKDEV(MAJOR_ADDR,0), 1)) < 0) {
++    printk(KERN_ERR "rckmem_init(): Could not allocate chrdev for rckmem!\n");
++    goto out_unalloc_region;
++  }
++
++  /* Initialize the device structure and register the "rckmpb" device (Minor 1) with the kernel */
++  cdev_init(&rckmpb_cdev, &rckmpb_fops);
++  if ((ret = cdev_add(&rckmpb_cdev, MKDEV(MAJOR_ADDR,1), 1)) < 0) {
++    printk(KERN_ERR "rckmem_init(): Could not allocate chrdev for rckmem!\n");
++    goto out_unalloc_rckncm;
++  }
++
++  /* Initialize the device structure and register the "rckdcm" device (Minor 2) with the kernel */
++  cdev_init(&rckdcm_cdev, &rckdcm_fops);
++  if ((ret = cdev_add(&rckdcm_cdev, MKDEV(MAJOR_ADDR,2), 1)) < 0) {
++    printk(KERN_ERR "rckmem_init(): Could not allocate chrdev for rckmem!\n");
++    goto out_unalloc_rckdcm;
++  }
++
++  /* Find out who we are and say hello... */
++  mappedAddr = ioremap_nocache(OWN_TILEID, sizeof(int));
++  if (mappedAddr) {
++    pid=readl(mappedAddr);
++    iounmap(mappedAddr);
++
++    /* Calculate Core number */
++    x=(pid>>3) & 0x0f; /* bits 06:03 */
++    y=(pid>>7) & 0x0f; /* bits 10:07 */
++    z=(pid   ) & 0x07; /* bits 02:00 */
++    PRINTD(KERN_INFO "rckmem_init():  -> We're on core %0d of tile x=%0d, y=%0d...\n", z, x, y);
++
++    /* Initialize (own) Messagepassing buffer... */
++    mappedAddr = ioremap_nocache(FIRST_MPB+(x*0x01000000)+(y*0x06000000), 2*MPBSIZE);
++    if (mappedAddr) {
++      for (loopMPB=z*MPBSIZE; loopMPB < ((z+1)*MPBSIZE); loopMPB+=4) {
++        writel(0x00000000, mappedAddr+loopMPB);
++      }
++      iounmap(mappedAddr);
++      PRINTD(KERN_INFO "rckmem_init(): -> Own MPB has been initialized with all zeros...\n");
++
++      /* Enable Messagepassing in CR4 */
++      set_in_cr4(_CR4_MPE);
++      PRINTD(KERN_INFO "rckmem_init(): -> Messagepassing has been enabled in CR4...\n");
++    }
++  }
++
++  return ret;
++  out_unalloc_rckdcm:
++  cdev_del(&rckdcm_cdev);
++  out_unalloc_rckncm:
++  cdev_del(&rckncm_cdev);
++  out_unalloc_region:
++  unregister_chrdev_region(MKDEV(MAJOR_ADDR,0), 1);
++  out:
++  return ret;
++}
++
++/* module unload */
++static void __exit rckmem_exit(void)
++{
++  PRINTD(KERN_INFO "rckmem_init(): Shutting down Rock Creek memory driver...\n");
++
++  /* remove the character device */
++  cdev_del(&rckdcm_cdev);
++  cdev_del(&rckncm_cdev);
++  cdev_del(&rckmpb_cdev);
++  unregister_chrdev_region(MKDEV(MAJOR_ADDR,0), 1);
++}
++
++module_init(rckmem_init);
++module_exit(rckmem_exit);
++MODULE_DESCRIPTION("Rock Creek system memory driver...");
++MODULE_AUTHOR("Michael Riepen <michael.riepen@intel.com>");
