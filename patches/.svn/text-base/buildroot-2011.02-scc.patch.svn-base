++/*
++ * Open and close
++ * These functions are called when an interface is activated/stopped. Thus,
++ * any system resources should be registered and the device itself should
++ * be initialized.
++ */
++static irqreturn_t rckmb_interrupt(int, void*);
++
++void rckmb_unmap(struct net_device* dev)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         i;
++
++  /* Note that there is just 1 CRB page per tile (=2 cores) */
++  for (i=0; i<RCK_CORECOUNT; i++) {
++    iounmap(priv->mpb[i]);
++    if (i%2) iounmap(priv->crb[i]);
++  }
++}
++
++int rckmb_open(struct net_device* dev)
++{
++  struct rckmb_priv*          priv = netdev_priv(dev);
++  int                         status = 0;
++  int                         i;
++  int                         address;
++  void*                       local_crb;
++  int                         tmp;
++  int                         x, y, z, pid;
++
++  /* MPB initialisation */
++  
++  /* Calculate how many cache lines are available for the circular buffer
++   * We need 1 status byte per core for message descriptors; in case
++   * support for multiple packets in-flight to the same destination is
++   * enabled, the descriptor range is expanded accordingly.
++   */
++  i = RCK_CORECOUNT/RCK_CLINE_SIZE;
++  if (multiPacket) i = 4*RCK_CORECOUNT/RCK_CLINE_SIZE;
++  if (i%RCK_CLINE_SIZE) i++;
++  priv->mpb_buffersize = (mpb_size/RCK_CLINE_SIZE) - i;
++      
++  /* Map all message buffers and config register space */
++  for (i=0; i<RCK_CORECOUNT; i++) {
++    /* Map the address directly, setting the PMB bit so the memory is indeed
++     * treated as message buffer by the core. Also mark it as Write-Through
++     * in order to avoid flushing the L1 cache to get the data into the
++     * message buffer.
++     */
++    address = rckmb_id2phys(i, _MPB);
++    priv->mpb[i] = ioremap_prot(address, mpb_size, _PAGE_PMB|_PAGE_PWT);
++    if (!priv->mpb[i]) status = -EIO;
++    
++    /* Note that there is just 1 CRB page per tile (=2 cores), i.e. we
++     * map the uncacheable region just once and copy it to the second core.
++     */
++    if (i%2 == 0) {
++      address = rckmb_id2phys(i, _CRB);
++      priv->crb[i] = ioremap_nocache(address, PAGE_SIZE);
++      if (!priv->crb[i]) status = -EIO;
++    }
++    else {
++      priv->crb[i] = priv->crb[i-1];
++    }
++  }
++
++  /* Also map the local CRB register space so we can determine our IP */
++  local_crb = ioremap_nocache(local_crb_offset, PAGE_SIZE);
++  if (!local_crb) status = -EIO;
++  
++  if (status) {
++    printk(KERN_WARNING "rckmb_open(): Can't map message passing buffer\n");
++    rckmb_unmap(dev);
++    iounmap(local_crb);
++    return status;
++  }
++
++  /* Determine the local IP address from the core number in the 
++   * tile ID register 
++   */
++  tmp = readl((void*)(local_crb + RCK_TILEID));
++  x = (tmp>>3) & 0x0f; /* bits 06:03 */
++  y = (tmp>>7) & 0x0f; /* bits 10:07 */
++  z = (tmp   ) & 0x07; /* bits 02:00 */
++  pid = 12*y + 2*x + z;
++  /* Add 1 to the processor ID to avoid *.*.*.0 IP addresses */
++  priv->localIp = pid + 1;
++  iounmap(local_crb);
++
++#ifdef DBG_PACKET_TRACE
++/* Map 2*1MB DDR3 memory per core as packet trace buffer */
++priv->packetTrace = ioremap_nocache(0x80800000ul + 0x400000ul*x + 0x200000ul*z, 2*1024*1024);
++memset(priv->packetTrace, 0, 2*1024*1024);
++#endif
++  printk(KERN_INFO "rckmb_open(): rckmb local IP = %d\n", priv->localIp);
++
++  /* Initialize the descriptor area located at the top of the message buffer */
++  address  = (int)(priv->mpb[priv->localIp-1]);
++  address += (RCK_CLINE_SIZE * priv->mpb_buffersize);
++  tmp = RCK_CORECOUNT;
++  if (multiPacket) tmp *= 4;
++  for (i=0; i<tmp; i++) *((u8*)(address+i)) = 0xFF;
++  rckmb_flush_wcbuffer(dev);
++    
++  /* Configure interrupt handling */
++  status = request_irq(dev->irq, &rckmb_interrupt, IRQF_SHARED, "rckmb", dev);
++  if (status) {
++    printk(KERN_WARNING "Can't get interrupt #%d\n", dev->irq);
++    rckmb_unmap(dev);
++    return status;
++  }
++
++  /* Assign the hardware address of the board (6 octets for Ethernet)
++   * Note that the first octet of ethernet multicast addresses is odd
++   */
++  memcpy(dev->dev_addr, "\0MPB_0", ETH_ALEN);
++    
++  netif_start_queue(dev);
++  
++  /* If interrupts are not used we immediately add the polling function
++   * to the queue which would otherwise be done through the IRQ handler.
++   */
++  //if (noIrq) netif_rx_schedule(dev);
++
++  return 0;
++}
++
++
++int rckmb_close(struct net_device* dev)
++{
++  /* Unmap the Rock Creek resources */
++  free_irq(dev->irq, dev);
++  rckmb_unmap(dev);
++  
++  netif_stop_queue(dev);
++  return 0;
++}
