+@@ -1008,6 +1008,8 @@
+ static int create_playback_stream(fd_info *i) {
+     pa_buffer_attr attr;
+     int n, flags;
++    int mute;
++    char *tmp = NULL;
+ 
+     assert(i);
+ 
+@@ -1029,6 +1031,21 @@
+     attr.minreq = (uint32_t) i->fragment_size;
