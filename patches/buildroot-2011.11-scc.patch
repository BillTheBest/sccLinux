+#!/bin/sh
+
+PROGRAM=/bin/hostname
+
+echo -n "Setting hostname: "
+[ -x ${PROGRAM} ] || (echo "missing"; exit 0)
+
+/usr/bin/pid > /dev/null 2>/dev/null
+if [ $? -eq 0 ]
+then
+	core=`pid --pid | sed s/pid=//`
+	HOST=rck$core
+else
+	HOST="rckemu"
+fi
+
+start() {
+	if ${PROGRAM} "${HOST}"; then
+		echo "'${HOST}'"
+	else
+		echo "failed"
+		exit 1
+	fi
+}
+
